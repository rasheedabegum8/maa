									<label for="searchable-select" class="col-form-label">Name</label>
									<input name="name" type="text" class="form-control" id="Uname" placeholder="Name"  pattern="^([A-Za-z]+ )+[A-Za-z]+$|^[A-Za-z]+$" required>
								</div>
								<div class="form-group">
									<label for="searchable-select" class="col-form-label">Email</label>
									<input name="email" type="text" class="form-control" id="Uemail" placeholder="Email" pattern="[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\.[A-Za-z]{2,63}$" required>
								</div>
								<div class="form-group">
									<label for="searchable-select" class="col-form-label">Phone</label>
									<input name="phone" type="text" class="form-control" id="Uphone" placeholder="Phone" minlength="10" maxlength="12"  pattern="^[0-9]*$" required>
