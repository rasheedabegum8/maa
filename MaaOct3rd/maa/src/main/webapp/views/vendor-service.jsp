										<select class="form-control" id="mtype">
											<option  value="">Select Mode</option>
											<option  value="Buy">Buy</option>
											<option  value="Sell">Sell</option>
											<option  value="Rent">Rent</option>
											<option  value="Tenancy">Tenancy Management</option>
