									<select class="form-control" name="propertyType" id="pType" data-placeholder="Select" data-enable-search="true" >
												<option value="">Select Property type</option>
												<option value="Apartments / Flats">Apartments/Flats</option>
												<option value="Independent houses/Villas">Independent houses / Villas</option>
												<option value="Commercial space">Commercial space</option>
